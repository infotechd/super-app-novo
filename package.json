{
  "name": "super-app",
  "private": true,
  "version": "1.0.0",
  "description": "Super App - Plataforma completa para conectar prestadores de serviÃ§o, compradores e anunciantes",
  "author": "Super App Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/infotechd/super-app-novo.git"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "start": "concurrently \"pnpm:start:backend\" \"pnpm:start:mobile\" \"pnpm:start:web\"",
    "start:mobile": "pnpm run start --filter packages/mobile",
    "start:web": "pnpm run start --filter packages/web",
    "start:backend": "pnpm run dev --filter packages/backend",
    "start:worker": "pnpm run start --filter packages/worker",
    "start:full": "concurrently \"pnpm:start:backend\" \"pnpm:start:worker\" \"pnpm:start:mobile\" \"pnpm:start:web\"",
    "android:mobile": "pnpm run android --filter packages/mobile",
    "ios:mobile": "pnpm run ios --filter packages/mobile",
    "build:mobile": "pnpm run build --filter packages/mobile",
    "build:web": "pnpm run build --filter packages/web",
    "build:backend": "pnpm run build --filter packages/backend",
    "build:common": "pnpm run build --filter packages/common",
    "build:worker": "pnpm run build --filter packages/worker",
    "build": "pnpm run build:common && pnpm run build:backend && pnpm run build:worker && pnpm run build:mobile && pnpm run build:web",
    "test:backend": "pnpm run test --filter packages/backend",
    "test:mobile": "pnpm run test --filter packages/mobile",
    "test:web": "pnpm run test --filter packages/web",
    "test:common": "pnpm run test --filter packages/common",
    "test:worker": "pnpm run test --filter packages/worker",
    "test": "pnpm run test:common && pnpm run test:backend && pnpm run test:worker && pnpm run test:mobile && pnpm run test:web"
  },
  "workspaces": [
    "packages/backend",
    "packages/mobile",
    "packages/web",
    "packages/common",
    "packages/worker"
  ],
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cookie-parser": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-preset-expo": "^12.0.11",
    "concurrently": "^8.2.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/core": "^7.10.0",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "axios": "^1.9.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "expo-router": "^4.0.20",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "stripe": "^18.0.0",
    "use-latest-callback": "^0.2.3"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
